  "author": "jason-dark",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "module-alias": "^2.2.3",
    "omelette": "^0.4.17",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/omelette": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3"

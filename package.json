  "description": "CLI tool for running AI prompts with structure and repeatability",
  "main": "dist/index.js",
  "bin": {
    "proompt": "./dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "./build.sh",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
